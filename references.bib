	pages = {800--833},
	file = {Mohajeri and Gudmundsson - 2012 - Entropies and Scaling Exponents of Street and Frac.pdf:/Users/geoff/Dropbox/Documents/School/Readings/Zotero/storage/E8HT83UX/Mohajeri and Gudmundsson - 2012 - Entropies and Scaling Exponents of Street and Frac.pdf:application/pdf},
