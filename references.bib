	volume = {59},
	issn = {1078-0874, 1552-8332},
	shorttitle = {The {Choice} to {Discriminate}},
	url = {http://journals.sagepub.com/doi/10.1177/10780874221109591},
	doi = {10.1177/10780874221109591},
	abstract = {The “choice” in the Housing Choice Voucher (HCV) program signals the mobility offered to voucher holders. However, some landlords use source of income (SOI) discrimination to exclude voucher holders—limiting their locational choices. We propose several factors likely to inﬂuence this landlord strategy including, market competitiveness, stereotypes, and racial discrimination. We examine the prevalence of express SOI discrimination and the effectiveness of SOI antidiscrimination laws. We utilize a novel dataset of 1,107,110 rental listings from the Craigslist pages of 77 mid-sized US cities. We ﬁnd signiﬁcant amounts of express SOI discrimination, even where there are SOI antidiscrimination laws. Using multilevel models, we ﬁnd that landlords are more likely to expressly discriminate in lower-opportunity neighborhoods and when they own market-competitive units. We also ﬁnd that these factors are moderated by the concentration of voucher holders. These ﬁndings underscore how landlord strategies can combine to undermine the choice afforded to voucher holders.},
	language = {en},
	number = {5},
	urldate = {2023-12-09},
	journal = {Urban Affairs Review},
	author = {Hangen, Forrest and O’Brien, Daniel T.},
	month = sep,
	year = {2023},
	pages = {1601--1625},
	file = {Hangen and O’Brien - 2023 - The Choice to Discriminate How Source of Income D.pdf:/Users/geoff/Dropbox/Documents/School/Readings/Zotero/storage/4JQDMUV3/Hangen and O’Brien - 2023 - The Choice to Discriminate How Source of Income D.pdf:application/pdf},
}

@article{schachter_inclusive_2023,
