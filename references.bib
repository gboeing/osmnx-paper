@article{jorgenson_power_2020,
	title = {Power, proximity, and physiology: does income inequality and racial composition amplify the impacts of air pollution on life expectancy in the {United} {States}?},
	volume = {15},
	issn = {1748-9326},
	shorttitle = {Power, proximity, and physiology},
	url = {https://iopscience.iop.org/article/10.1088/1748-9326/ab6789},
	doi = {10.1088/1748-9326/ab6789},
	abstract = {This study advances research at the intersection of environmental degradation, social stratiﬁcation, and population health in the United States. Expanding the theoretical principles of power, proximity, and physiology, we hypothesize that the harmful effect of ﬁne particulate matter on life expectancy is greater in states with higher levels of income inequality and larger black populations. To test our hypothesis, we use two-way ﬁxed effects regression analysis to estimate the effect of a three-way interaction between ﬁne particulate matter, income share of the top ten percent, and the percent of the population that is black on state-level average life expectancy for all US states and the District of Columbia (2000–2014). The ﬁndings support our hypothesis: the estimated effect of the three-way interaction on average life expectancy is negative and statistically signiﬁcant, net of various socioeconomic and demographic controls. Using post-estimation techniques, we visually illustrate that the harmful effect of ﬁne particulate matter on life expectancy is especially pronounced in states with both very high levels of income inequality and very large black populations. We conclude by summarizing the theoretical and substantive implications of our ﬁndings, the limitations of the study, and potential next steps in this evolving area of interdisciplinary research.},
	language = {en},
	number = {2},
	urldate = {2020-02-26},
	journal = {Environmental Research Letters},
	author = {Jorgenson, Andrew K and Hill, Terrence D and Clark, Brett and Thombs, Ryan P and Ore, Peter and Balistreri, Kelly S and Givens, Jennifer E},
	month = feb,
	year = {2020},
	pages = {024013},
	file = {Jorgenson et al. - 2020 - Power, proximity, and physiology does income ineq.pdf:/Users/geoff/Dropbox/Documents/School/Readings/Zotero/storage/97RLC5NF/Jorgenson et al. - 2020 - Power, proximity, and physiology does income ineq.pdf:application/pdf},
